<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor" >

    <ImageView
        android:id="@+id/traffic_main_line_00"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="#BABABA" />

    <RelativeLayout
        android:id="@+id/traffic_main_dwkc"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/traffic_main_line_00"
        android:background="@color/backgroundColor_low"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true" >

        <ImageView
            android:id="@+id/traffic_main_dwkc_img"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/setting_version" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/traffic_main_dwkc_img"
            android:clickable="false"
            android:focusable="false"
            android:text="点位勘察"
            android:textColor="#000000"
            android:textSize="15dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/ic_right_arrow" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/traffic_main_line_01"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/traffic_main_dwkc"
        android:layout_marginLeft="10dp"
        android:background="#BABABA" />

    <RelativeLayout
        android:id="@+id/traffic_main_jckw"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/traffic_main_line_01"
        android:background="@color/backgroundColor_low"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true" >

        <ImageView
            android:id="@+id/traffic_main_jckw_img"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/setting_version" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/traffic_main_jckw_img"
            android:clickable="false"
            android:focusable="false"
            android:text="基础开挖"
            android:textColor="#000000"
            android:textSize="15dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/ic_right_arrow" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/traffic_main_line_02"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/traffic_main_jckw"
        android:layout_marginLeft="10dp"
        android:background="#BABABA" />

    <RelativeLayout
        android:id="@+id/traffic_main_jcjz"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/traffic_main_line_02"
        android:background="@color/backgroundColor_low"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true" >

        <ImageView
            android:id="@+id/traffic_main_jcjz_img"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/setting_version" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/traffic_main_jcjz_img"
            android:clickable="false"
            android:focusable="false"
            android:text="基础浇筑"
            android:textColor="#000000"
            android:textSize="15dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/ic_right_arrow" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/traffic_main_line_03"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/traffic_main_jcjz"
        android:layout_marginLeft="10dp"
        android:background="#BABABA" />

    <RelativeLayout
        android:id="@+id/traffic_main_dhys"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/traffic_main_line_03"
        android:background="@color/backgroundColor_low"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true" >

        <ImageView
            android:id="@+id/traffic_main_dhys_img"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/setting_version" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/traffic_main_dhys_img"
            android:clickable="false"
            android:focusable="false"
            android:text="到货验收"
            android:textColor="#000000"
            android:textSize="15dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/ic_right_arrow" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/traffic_main_line_04"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/traffic_main_dhys"
        android:layout_marginLeft="10dp"
        android:background="#BABABA" />

    <RelativeLayout
        android:id="@+id/traffic_main_qita"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/traffic_main_line_04"
        android:background="@color/backgroundColor_low"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true" >

        <ImageView
            android:id="@+id/traffic_main_qita_img"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/setting_version" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/traffic_main_qita_img"
            android:clickable="false"
            android:focusable="false"
            android:text="其他"
            android:textColor="#000000"
            android:textSize="15dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/ic_right_arrow" />
    </RelativeLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/traffic_main_qita"
        android:background="#BABABA" />

</RelativeLayout>